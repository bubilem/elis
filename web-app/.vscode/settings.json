{
  "workbench.colorTheme": "Default Dark+",
  "window.zoomLevel": 2,
  "editor.fontSize": 15,
  "terminal.integrated.fontSize": 12,
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "liveSassCompile.settings.formats": [
    {
      "format": "compressed",
      "extensionName": ".min.css",
      "savePath": "/www/css"
